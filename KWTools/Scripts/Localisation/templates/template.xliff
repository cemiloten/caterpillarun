<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 http://docs.oasis-open.org/xliff/v1.2/os/xliff-core-1.2-strict.xsd">
  <file original="{{ master_language }}.lproj/InfoPlist.strings" source-language="en" target-language="{{ header_language }}" datatype="plaintext">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="11.0" build-num="11A420a"/>
    </header>
    <body>{% for loop_data in loop_data_array %}
      <trans-unit id="{{ loop_data.id }}" xml:space="preserve">
        <source>{{ loop_data.name }}</source>
        <target>{{ loop_data.target }}</target>
        <note>Bundle display name</note>
      </trans-unit> {% endfor %}
    </body>
  </file>
</xliff>
